{
    "name": "snowsoft/composer-viewer",
    "description": "Composer Viewer for Laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/snowsoft/composer-viewer",
    "license": "MIT",
    "authors": [
        {
            "name": "snowsoft",
            "email": "abdulkadirkar@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "snowsoft/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Jxlwqq\\ComposerViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jxlwqq\\ComposerViewer\\ComposerViewerServiceProvider"
            ]

        }
    }
}
